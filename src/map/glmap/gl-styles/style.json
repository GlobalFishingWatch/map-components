      }
    },
    {
      "id": "events_port_vessel_number",
      "source": "events_port_vessel",
      "type": "symbol",
      "metadata": {
        "mapbox:group": "temporal"
      },
      "filter": ["has", "point_count"],
      "layout": {
        "text-field": "{point_count_abbreviated}",
        "text-font": ["Roboto Medium"],
