      "metadata": {
        "mapbox:group": "static"
      },
      "type": "fill",
      "source": "falklands_conservation",
      "source-layer": "falklands_conservation"
    },
    {
      "id": "falklands_conservation-labels",
      "metadata": {
        "gfw:isLabel": true,
        "mapbox:group": "static"
      },
      "type": "symbol",
