    {
      "id": "highseas-labels",
      "metadata": {
        "gfw:isLabel": true,
        "mapbox:group": "static"
      },
      "type": "symbol",
      "source": "highseas",
      "source-layer": "highseas",
      "layout": {
        "text-field": "{regionid}",
        "text-font": ["Roboto Mono Light"],
